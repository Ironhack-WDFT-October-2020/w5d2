<h2>Edit a Movie</h2>
<form action="/movies/{{ movieData._id}}" method="POST">
  <label for="title">title</label> 
  <input id="title" type="text" name="title" value="{{ movieData.title}}">
  
  <label for="genre">Genre</label> 
  <input id="genre" type="text" name="genre" value="{{ movieData.genre}}">
  
  <label for="plot">Plot</label> 
  <input id="plot" type="text" name="plot" value="{{ movieData.plot}}">
  
  <button type="submit">Update</button>
</form>