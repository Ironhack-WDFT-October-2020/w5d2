<form action="/movies" method="POST"> 
    <input type="text" name = "title">
    <input type="text" name = "genre">
    <input type="text" name = "plot">
    
    <button type="submit">Add the movie</button>
   
     <select name="celebrityName" id="celebSelect"  multiple>
         {{#each celebData}}
